    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/ui": "^2.1.8",
    "@vue/tsconfig": "^0.5.1",
    "async-validator": "^4.2.5",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.2.0",
    "npm-run-all": "^4.1.5",
