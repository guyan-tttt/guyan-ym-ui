{
  "name": "@ym-UI/WE",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @ym-UI/play dev",
    "docs:dev": "pnpm --filter @ym-UI/docs dev",
    "docs:build": "pnpm --filter @ym-UI/docs build",
    "docs:preview": "pnpm --filter @ym-UI/docs preview",
    "docs:move": "node scripts/docs-publish.js",
    "docs:publish": "run-s docs:build docs:move",
    "test": "run-p test:utils test:comp",
    "test:comp": " cross-env NODE_ENV=test pnpm --filter @ym-UI/components test",
    "test:utils": "cross-env NODE_ENV=test pnpm --filter @ym-UI/utils test",
    "test:ui": "run-p test-ui:utils test-ui:comp",
    "test-ui:comp": "cross-env NODE_ENV=test pnpm --filter @ym-UI/components test",
    "test-ui:utils": "cross-env NODE_ENV=test pnpm --filter @ym-UI/utils test:ui",
    "storybook": "pnpm --filter @ym-UI/play storybook",
    "build": "run-s build:hooks build:comp",
    "build:comp": "cross-env NODE_ENV=production pnpm --filter guyan-ym-ui build",
    "build:dev": "cross-env NODE_ENV=development pnpm --filter guyan-ym-ui build:watch",
    "build:hooks": "cross-env NODE_ENV=production pnpm --filter @ym-UI/hooks build",
    "release": "pnpm -filter guyan-ym-ui release",
    "patch:publish": "pnpm -filter guyan-ym-ui patch:publish",
    "publish": "run-s build patch:publish"
  },
